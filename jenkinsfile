node {
    
    stage ("Get Parameters") {
        checkout scm
        List params = [
            string(name: 'Instance_ID', description: 'Please Enter the Instance ID.')
        ]
     }


pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}

}